{
    "typeAcquisition": {
        "enable": true
    },
    
    "compileOnSave": false,
    "compilerOptions": {
        "sourceMap": true,
        "module": "commonjs",
        "target": "es2015",
        "noImplicitAny": true,
        "jsx": "react",
        "outDir": "wwwroot/dist",
        "moduleResolution": "node",
        "esModuleInterop": true,
        "types": ["@types/webpack-env"] // Typescript compilation 
    },
    "exclude": [
        "node_modules",
        "wwwroot"
    ]
}
